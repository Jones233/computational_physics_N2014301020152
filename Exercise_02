import os
for i in range(0,30):
    print (i*' ' +' ###  #   # ##### ####  #     ##### #     #')
    print (i*' ' +'#     #   #   #   #   # #     #      #   # ')
    print (i*' ' +' ###  #####   #   ####  #     #####    #   ')
    print (i*' ' +'    # #   #   #   # #   #     #        #   ')
    print (i*' ' +' ###  #   # ##### #  ## ##### #####    #   ')
    os.system('cls')

